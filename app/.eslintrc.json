{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "prettier",
      "import"
  ],
  "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto",
          "singleQuote": false
        }
      ]
  }
}